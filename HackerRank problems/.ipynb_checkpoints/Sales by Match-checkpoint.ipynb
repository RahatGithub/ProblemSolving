   "source": [
    "from collections import Counter \n",
    "\n",
    "n = int(input())\n",
    "colors = Counter(map(int, input().split()))\n",
    "pairs = 0\n",
    "for c in colors:\n",
    "    pairs += colors[c]//2\n",
    "print('result: ',pairs)"
   ]
